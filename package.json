{
    "name": "expo-redux-logger",
    "version": "1.0.0",
    "description": "redux logger for Expo project",
    "repository": {
        "type": "git",
        "url": "https://github.com/11grossmane/expo-redux-logger.git"
    },
    "main": "./lib/index.js",
    "husky": {
        "hooks": {
            "pre-commit": "npm run cleanbuild && prettier * --write"
        }
