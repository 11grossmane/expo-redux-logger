    "license": "ISC",
    "dependencies": {
        "redux": "^4.0.5"
    },
    "devDependencies": {
        "@types/node": "^13.13.42",
        "@types/redux": "^3.6.0",
        "@types/typescript": "^2.0.0",
        "husky": "^5.0.9",
        "ts-node": "^8.9.0",
        "typescript": "^3.8.3"
    }
}
